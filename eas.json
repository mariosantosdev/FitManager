{
  "cli": {
    "version": ">= 0.42.4"
  },
  "build": {
    "development": {
      "distribution": "internal",
      "android": {
        "gradleCommand": ":app:assembleDebug"
      },
      "ios": {
        "buildConfiguration": "Debug"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "DEV": "false",
        "BASE_URL": "https://fitmanager-server.herokuapp.com/"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
